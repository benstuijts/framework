<% var longstory = data.longstory %>
<section id="our-long-story" class="our-long-story">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2><%= longstory.header %></h2>
                <hr class="intro-divider">
            </div>
            <div class="col-lg-12">
                <p class="lead">
                    <%= longstory.leadparagraph %>    
                </p>
                <p>
                    <%= longstory.body %>
                </p>
            </div>
            <% include share %>
        </div>
    </div>
</section>