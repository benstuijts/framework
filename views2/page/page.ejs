<% include ../templates/header %>
<% include ../templates/navigation %>

<input type="hidden" value="<%= page.image %>" id="page.image">



<div class="intro-header" id="intro-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="intro-message">
                        
                        <h1><%= page.header %></h1>
                        <h3><%= page.subheader %></h3>
 
                    </div>
                </div>
            </div>

        </div>
        <!-- /.container -->
</div>


<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <i class="fa fa-calendar" aria-hidden="true"></i> <%= page.date %> 
            <div class="pull-right">
                Delen...  
                <a href="http://www.facebook.com/sharer.php?u=<%= page.url %>" class="share-square-part share-square-facebook">
                    <i class="fa fa-facebook-square fa-2x fa-fw"></i>
                </a>
                <a href="http://twitter.com/share?url=<%= page.url %>" class="share-square-part share-square-twitter">
                    <i class="fa fa-twitter fa-2x fa-fw text-info"></i>
                </a>
                <a href="https://plus.google.com/share?url=<%= page.url %>" class="bg-warning share-square-part share-square-google">
                    <i class="fa fa-google fa-2x fa-fw text-danger"></i>
                </a>
                <a href="https://www.linkedin.com/shareArticle?mini=true&url=<%= page.url %>" class="share-square-part share-square-linkedin">
                    <i class="fa fa-linkedin fa-2x fa-fw"></i>
                </a>
            </div>
        </div>
    </div>

    <article>
        <%- page.body %>
    </article>
</div>


<% include ../templates/footer %>