<% var longstory = data.longstory %>
<section id="our-long-story" class="our-long-story">
    <article itemscope itemtype="http://schema.org/Article" class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 itemprop="name"><%= longstory.header %></h2>
                <hr class="intro-divider">
            </div>
            <div class="col-lg-12" itemprop="articleBody">
                <p class="lead">
                    <%= longstory.leadparagraph %>    
                </p>
                <p>
                    <%= longstory.body %>
                </p>
            </div>
            <% include share %>
        </div>
    </article>
</section>